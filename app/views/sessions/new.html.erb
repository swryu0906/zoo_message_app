<h1>Log in</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <div class="">
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div class="">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div class="">
    <%= f.submit "Log in" %>
  </div>
<% end %>
